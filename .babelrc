{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "markdown-in-js/babel",
    "react-intl"
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "inline-dotenv",
          {
            "path": ".env.dev"
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "inline-dotenv"
        ]
      ]
    }
  }
}
