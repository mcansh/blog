{
  "project": "blog",
  "version": 2,
  "regions": ["all"],
  "routes": [
    {
      "src": "(.*)",
      "headers": {
        "Content-Security-Policy": "default-src 'none';script-src 'self' https://polyfill.io/v3/polyfill.min.js www.googletagmanager.com www.google-analytics.com https://storage.googleapis.com https://cdn.ampproject.org/v0.js;connect-src 'self' ws://localhost:* https://sentry.io/ www.google-analytics.com https://res.cloudinary.com/dof0zryca;manifest-src 'self' data:;style-src 'self' 'unsafe-inline';img-src 'self' https://res.cloudinary.com/dof0zryca/ data: www.googletagmanager.com www.google-analytics.com;report-uri https://sentry.io/api/197817/security/?sentry_key=07a54d3b59bb4bf5ad1c6ddf050d51c1"
      },
      "continue": true
    },
    {
      "src": "/changelog",
      "status": 301,
      "headers": {
        "Location": "https://github.com/mcansh/blog/releases"
      }
    },
    {
      "src": "^/(manifest.webmanifest|manifest.json)$",
      "dest": "/manifest.webmanifest",
      "headers": {
        "content-type": "application/manifest+json",
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/(manifest.amp.webmanifest|manifest.amp.json)",
      "headers": {
        "content-type": "application/manifest+json",
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "^/(favicon.ico|feed.json|sitemap.xml)$",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/atom",
      "dest": "/atom.xml",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/sw.js",
      "dest": "/_next/static/sw.js",
      "headers": {
        "cache-control": "max-age=0",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/_next(/static/(?:[^/]+/pages|chunks|runtime)/.+)",
      "headers": {
        "cache-control": "immutable,max-age=31536000"
      }
    }
  ],
  "github": {
    "silent": true
  }
}
