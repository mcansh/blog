{
  "project": "blog",
  "version": 2,
  "alias": [
    "www.mcansh.blog",
    "mcansh.blog"
  ],
  "regions": [
    "all"
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next@canary",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/changelog",
      "status": 301,
      "headers": {
        "Location": "https://github.com/mcansh/blog/releases"
      }
    },
    {
      "src": "^/(manifest.webmanifest|manifest.json)$",
      "dest": "/manifest.webmanifest",
      "headers": {
        "content-type": "application/manifest+json",
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/manifest.amp.webmanifest",
      "headers": {
        "content-type": "application/manifest+json",
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "^/(favicon.ico|logo.png|feed.json|sitemap.xml)$",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/atom",
      "dest": "/atom.xml",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/sw.js",
      "dest": "/_next/static/sw.js",
      "headers": {
        "cache-control": "max-age=0",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
