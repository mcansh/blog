{
  "name": "blog",
  "productName": "Logan McAnsh",
  "productShortName": "Blog",
  "version": "3.5.6",
  "private": false,
  "description": "My blog for learn.co",
  "repository": "mcansh/blog",
  "homepage": "https://mcansh.blog",
  "scripts": {
    "dev": "next",
    "dev:server": "nodemon -r esm server",
    "build": "next build && node ./scripts/default-lang",
    "compress": "node ./scripts/compress.js",
    "start": "node -r esm server",
    "lint": "eslint ./ --quiet",
    "test": "bundlesize && yarn lint",
    "predeploy": "yarn compress",
    "deploy": "now --docker",
    "postdeploy": "now alias && now rm blog --safe -y && now scale mcansh.blog 1"
  },
  "precommit": "lint",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "dependencies": {
    "accepts": "1.3.5",
    "apollo-boost": "0.1.3",
    "apollo-client": "2.2.7",
    "babel-plugin-inline-dotenv": "1.1.2",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-styled-components": "1.5.1",
    "date-fns": "2.0.0-alpha.7",
    "esm": "3.0.7",
    "glob": "7.1.2",
    "graphql": "0.13.2",
    "highlight.js": "9.12.0",
    "intl": "1.2.5",
    "isomorphic-unfetch": "2.0.0",
    "markdown-in-js": "1.1.3",
    "next": "5.0.1-canary.16",
    "next-workbox-webpack-plugin": "1.1.0",
    "nprogress": "0.2.0",
    "polka": "0.3.4",
    "prop-types": "15.6.1",
    "raven-js": "3.23.3",
    "react": "16.3.0-alpha.1",
    "react-apollo": "2.1.0-beta.3",
    "react-dom": "16.3.0-alpha.1",
    "react-ga": "2.4.1",
    "react-intl": "2.4.0",
    "react-syntax-highlighter": "7.0.2",
    "remark": "9.0.0",
    "remark-emoji": "2.0.1",
    "remark-github": "7.0.3",
    "remark-react": "4.0.1",
    "styled-components": "3.2.3",
    "unstated": "1.1.0",
    "webpack": "4.2.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "bundlesize": "0.17.0",
    "eslint": "4.19.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-mcansh": "1.0.3",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "imagemin": "5.3.1",
    "imagemin-mozjpeg": "7.0.0",
    "imagemin-pngquant": "5.1.0",
    "imagemin-webp": "4.1.0",
    "mime-types": "2.1.18",
    "nodemon": "1.17.2",
    "pre-commit": "1.2.2",
    "prettier": "1.11.1",
    "sharp": "0.20.1"
  },
  "esm": {
    "mode": "auto",
    "cjs": true,
    "await": true
  },
  "bundlesize": [
    {
      "path": "./pages/**/*.js"
    }
  ],
  "nodemon": {
    "watch": "server.js"
  }
}
