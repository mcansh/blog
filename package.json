{
  "name": "mcansh-blog",
  "productName": "Logan McAnsh",
  "productShortName": "Blog",
  "version": "3.1.9",
  "description": "My blog for learn.co",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "webp": "node convertToWebp.js",
    "start": "NODE_ENV=production node -r @std/esm server",
    "lint": "eslint ./",
    "test": "bundlesize && yarn lint",
    "prebuild": "yarn webp",
    "prebuild:docker": "yarn webp",
    "predeploy": "yarn webp",
    "build:docker": "docker build -t blog .",
    "start:docker": "docker run -p 3000:3000 blog",
    "deploy": "now --docker && now alias"
  },
  "precommit": [
    "lint"
  ],
  "now": {
    "name": "blog",
    "alias": [
      "mcansh.blog"
    ],
    "dotenv": "variables.env"
  },
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "dependencies": {
    "@std/esm": "^0.18.0",
    "babel-plugin-inline-dotenv": "^1.1.1",
    "date-fns": "^1.29.0",
    "highlight.js": "^9.12.0",
    "markdown-in-js": "^1.1.3",
    "next": "^4.3.0-canary.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.0",
    "raven-js": "^3.21.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-syntax-highlighter": "^6.1.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "bundlesize": "^0.15.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "file-type": "^7.4.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "read-chunk": "^2.1.0",
    "webp-converter": "^2.1.6"
  },
  "@std/esm": {
    "esm": "all",
    "cjs": true
  },
  "bundlesize": [
    {
      "path": "./pages/**/*.js"
    }
  ],
  "repository": "git@github.com:mcansh/blog.git",
  "private": false
}
