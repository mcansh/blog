{
  "name": "mcansh-blog",
  "productName": "Logan McAnsh",
  "productShortName": "Blog",
  "version": "3.3.6-hotttt",
  "private": false,
  "description": "My blog for learn.co",
  "repository": "git@github.com:mcansh/blog.git",
  "homepage": "https://mcansh.blog",
  "scripts": {
    "dev": "next",
    "dev:server": "nodemon -r @std/esm server",
    "build": "next build && node ./scripts/default-lang",
    "compress": "node ./scripts/compress.js",
    "start": "node -r @std/esm server",
    "lint": "eslint ./",
    "test": "bundlesize && yarn lint",
    "predeploy": "yarn compress",
    "deploy": "now --docker",
    "postdeploy": "now alias && now rm blog --safe -y && now scale mcansh.blog 1"
  },
  "precommit": "lint",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "dependencies": {
    "@pwa/next-workbox-webpack-plugin": "0.4.1",
    "@std/esm": "0.23.1",
    "accepts": "1.3.4",
    "babel-plugin-inline-dotenv": "1.1.2",
    "babel-plugin-react-intl": "2.4.0",
    "date-fns": "2.0.0-alpha.7",
    "glob": "7.1.2",
    "highlight.js": "9.12.0",
    "intl": "1.2.5",
    "markdown-in-js": "1.1.3",
    "next": "5.0.1-canary.7",
    "nprogress": "0.2.0",
    "polka": "0.3.4",
    "prop-types": "15.6.0",
    "raven-js": "3.22.3",
    "react": "16.3.0-alpha.1",
    "react-dom": "16.3.0-alpha.1",
    "react-ga": "2.4.1",
    "react-intl": "2.4.0",
    "react-syntax-highlighter": "7.0.1",
    "unistore": "3.0.4",
    "webpack": "4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "bundlesize": "0.16.0",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "imagemin": "5.3.1",
    "imagemin-mozjpeg": "7.0.0",
    "imagemin-pngquant": "5.0.1",
    "imagemin-webp": "4.1.0",
    "nodemon": "1.15.1",
    "pre-commit": "1.2.2",
    "prettier": "1.10.2"
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true,
    "await": true
  },
  "bundlesize": [
    {
      "path": "./pages/**/*.js"
    }
  ],
  "nodemon": {
    "watch": "server.js"
  }
}
