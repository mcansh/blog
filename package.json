{
  "name": "blog",
  "productName": "Logan McAnsh",
  "productShortName": "Blog",
  "version": "3.11.2",
  "private": false,
  "description": "My blog for learn.co, but mostly it's a playground for things I want to build with React ü§∑‚Äç‚ôÇÔ∏è",
  "repository": "mcansh/blog",
  "homepage": "https://mcansh.blog",
  "browser": {
    "@sentry/node": "@sentry/browser",
    "path": "isomorphic-path"
  },
  "scripts": {
    "dev": "next",
    "dev:server": "nodemon -r dotenv/config -r esm -w server server",
    "compress": "node -r esm ./scripts/compress.js",
    "build": "next build",
    "postbuild": "node -r esm ./scripts/default-lang",
    "start": "node -r dotenv/config -r esm server",
    "predeploy": "yarn compress",
    "deploy": "now",
    "postdeploy": "now alias && now rm blog -ys && now scale mcansh.blog 1",
    "test": "NODE_ENV=test jest",
    "lint:css": "stylelint './'",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "analyze:both": "BUNDLE_ANALYZE=both yarn build",
    "blog": "node -r esm scripts/create-post",
    "flow": "flow",
    "flow:install": "yarn flow-typed install"
  },
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "dependencies": {
    "@mdx-js/mdx": "0.16.6",
    "@mdx-js/tag": "0.16.6",
    "@polka/send-type": "0.5.0",
    "@sentry/browser": "4.5.0",
    "@sentry/node": "4.5.0",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "@zeit/next-mdx": "1.2.0",
    "@zeit/next-source-maps": "0.0.3",
    "accepts": "1.3.5",
    "apollo-boost": "0.1.23",
    "babel-plugin-inline-dotenv": "1.2.0",
    "babel-plugin-polished": "1.1.0",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-styled-components": "1.10.0",
    "common-tags": "1.8.0",
    "dotenv": "6.2.0",
    "esm": "3.0.84",
    "glob": "7.1.3",
    "graphql": "14.0.2",
    "hoist-non-react-statics": "3.2.1",
    "intl": "1.2.5",
    "is-absolute-url": "2.1.0",
    "isomorphic-path": "1.0.0",
    "isomorphic-unfetch": "3.0.0",
    "mime": "2.4.0",
    "next": "8.0.0-canary.6",
    "next-compose-plugins": "2.1.1",
    "next-offline": "3.3.3",
    "next-server": "8.0.0-canary.6",
    "next-size": "1.0.1",
    "next-with-apollo": "3.3.1",
    "nprogress": "0.2.0",
    "polished": "2.3.3",
    "polka": "0.5.1",
    "prism-react-renderer": "0.1.5",
    "react": "16.8.0-alpha.0",
    "react-adopt": "0.6.0",
    "react-apollo": "2.3.3",
    "react-dom": "16.8.0-alpha.0",
    "react-ga": "2.5.6",
    "react-infinite-scroller": "1.2.4",
    "react-intl": "2.7.2",
    "react-spring": "7.2.8",
    "react-syntax-highlighter": "10.1.2",
    "remark": "10.0.1",
    "remark-emoji": "2.0.2",
    "remark-github": "7.0.6",
    "remark-react": "5.0.1",
    "serve-favicon": "2.5.0",
    "sitemap": "2.1.0",
    "somebody": "2.0.0",
    "styled-components": "4.1.3",
    "styled-reset": "1.6.4",
    "sw-precache-webpack-plugin": "0.11.5",
    "tinydate": "1.0.0",
    "tmp-cache": "1.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/preset-flow": "7.0.0",
    "@sindresorhus/slugify": "0.7.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-mcansh": "1.0.9",
    "eslint-config-prettier": "3.4.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.3",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.3",
    "flow-bin": "0.90.0",
    "flow-typed": "2.5.1",
    "husky": "1.3.1",
    "imagemin": "6.1.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "7.0.0",
    "imagemin-webp": "5.0.0",
    "inquirer": "6.2.1",
    "inquirer-path": "1.0.0-beta5",
    "jest": "23.6.0",
    "jest-styled-components": "6.3.1",
    "kleur": "3.0.1",
    "lint-staged": "8.1.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.7.0",
    "react-testing-library": "5.4.4",
    "stylelint": "9.9.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.1",
    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "3.0.3"
  },
  "esm": {
    "mode": "auto",
    "cjs": true,
    "await": true
  },
  "greenkeeper": {
    "commitMessages": {
      "initialBadge": ":memo: Docs: Add Greenkeeper badge",
      "initialDependencies": ":gem: Upgrade: Update dependencies",
      "initialBranches": ":tada: Build: Whitelist greenkeeper branches",
      "dependencyUpdate": ":gem: Upgrade: Update ${dependency} to version ${version}",
      "devDependencyUpdate": ":gem: Upgrade: Update ${dependency} to version ${version}",
      "dependencyPin": ":bug: Fix: Pin ${dependency} to ${oldVersion}",
      "devDependencyPin": ":bug: Fix: Pin ${dependency} to ${oldVersion}"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn flow focus-check",
      "stylelint",
      "eslint --fix",
      "git add"
    ]
  }
}
