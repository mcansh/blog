{
  "name": "blog",
  "productName": "Logan McAnsh",
  "productShortName": "Blog",
  "version": "3.12.0",
  "private": false,
  "description": "My blog for learn.co, but mostly it's a playground for things I want to build with React ü§∑‚Äç‚ôÇÔ∏è",
  "repository": "mcansh/blog",
  "homepage": "https://mcansh.blog",
  "browser": {
    "@sentry/node": "@sentry/browser",
    "path": "isomorphic-path"
  },
  "scripts": {
    "dev": "next",
    "compress": "node -r esm ./scripts/compress.js",
    "build": "next build",
    "build:files": "yarn ts-node --project tsconfig.server.json ./scripts/build/",
    "start": "next start",
    "predeploy": "yarn compress",
    "deploy": "now",
    "postdeploy": "now alias && now rm blog -ys",
    "test": "NODE_ENV=test jest",
    "lint:css": "stylelint './'",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "analyze:both": "BUNDLE_ANALYZE=both yarn build",
    "blog": "node -r esm scripts/create-post",
    "type-check": "tsc"
  },
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
