{
  "name": "blog",
  "productName": "Logan McAnsh",
  "productShortName": "Blog",
  "version": "3.10.5",
  "private": false,
  "description": "My blog for learn.co, but mostly it's a play ground for things I want to build with React ü§∑‚Äç‚ôÇÔ∏è",
  "repository": "mcansh/blog",
  "homepage": "https://mcansh.blog",
  "browser": {
    "raven": "raven-js"
  },
  "scripts": {
    "dev": "next",
    "dev:server": "nodemon -r esm -w server server",
    "compress": "node -r esm ./scripts/compress.js",
    "build": "next build",
    "postbuild": "node -r esm ./scripts/default-lang",
    "start": "node -r esm server",
    "predeploy": "yarn compress",
    "deploy": "now -E .env",
    "postdeploy": "now alias && now rm blog -ys && now scale mcansh.blog sfo1 1",
    "git-pre-commit": "yarn lint && yarn test",
    "test": "NODE_ENV=test jest",
    "lint": "eslint ./ --quiet --no-ignore .eslintrc.js .size-limit.js",
    "lint:css": "stylelint './components'",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "analyze:both": "BUNDLE_ANALYZE=both yarn build",
    "size": "yarn build && size-limit",
    "blog": "node -r esm scripts/create-post"
  },
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "dependencies": {
    "@mdx-js/mdx": "0.15.0-0",
    "@mdx-js/tag": "0.14.1",
    "@zeit/next-bundle-analyzer": "0.1.1",
    "@zeit/next-mdx": "1.1.0",
    "@zeit/next-source-maps": "0.0.2",
    "accepts": "1.3.5",
    "apollo-boost": "0.1.12",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-styled-components": "1.5.1",
    "esm": "3.0.72",
    "glob": "7.1.2",
    "graphql": "0.13.2",
    "highlight.js": "9.12.0",
    "http-status-codes": "1.3.0",
    "intl": "1.2.5",
    "is-absolute-url": "2.1.0",
    "isomorphic-unfetch": "2.1.0",
    "mime": "2.3.1",
    "mz": "2.7.0",
    "next": "6.1.1",
    "next-compose-plugins": "2.1.1",
    "next-offline": "2.8.0",
    "nprogress": "0.2.0",
    "polished": "1.9.3",
    "polka": "0.4.0",
    "prop-types": "15.6.2",
    "prop-types-exact": "1.2.0",
    "raven": "2.6.3",
    "raven-js": "3.26.4",
    "react": "16.4.1",
    "react-apollo": "2.1.9",
    "react-dom": "16.4.1",
    "react-ga": "2.5.3",
    "react-infinite-scroller": "1.2.0",
    "react-intl": "2.4.0",
    "react-syntax-highlighter": "8.0.1",
    "remark": "9.0.0",
    "remark-emoji": "2.0.1",
    "remark-github": "7.0.3",
    "remark-react": "4.0.3",
    "serve-favicon": "2.5.0",
    "sitemap": "1.13.0",
    "size-limit": "0.19.0",
    "somebody": "2.0.0",
    "styled-components": "3.3.3",
    "tinydate": "1.0.0",
    "tmp-cache": "1.0.0",
    "webpack": "3.12.0"
  },
  "devDependencies": {
    "@zeit/git-hooks": "0.1.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "common-tags": "1.8.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.2.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-mcansh": "1.0.7",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.10.0",
    "fs-extra": "7.0.0",
    "imagemin": "6.0.0",
    "imagemin-mozjpeg": "7.0.0",
    "imagemin-pngquant": "6.0.0",
    "imagemin-webp": "4.1.0",
    "inquirer": "6.0.0",
    "inquirer-path": "1.0.0-beta5",
    "jest": "23.4.1",
    "jest-styled-components": "5.0.1",
    "kleur": "2.0.1",
    "nodemon": "1.18.3",
    "now-env": "3.1.0",
    "prettier": "1.13.7",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.4.1",
    "stylelint": "9.4.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.1",
    "webpack-bundle-analyzer": "2.13.1"
  },
  "esm": {
    "mode": "auto",
    "cjs": true,
    "await": true
  },
  "greenkeeper": {
    "commitMessages": {
      "initialBadge": ":memo: Docs: Add Greenkeeper badge",
      "initialDependencies": ":gem: Upgrade: Update dependencies",
      "initialBranches": ":tada: Build: Whitelist greenkeeper branches",
      "dependencyUpdate": ":gem: Upgrade: Update ${dependency} to version ${version}",
      "devDependencyUpdate": ":gem: Upgrade: Update ${dependency} to version ${version}",
      "dependencyPin": ":bug: Fix: Pin ${dependency} to ${oldVersion}",
      "devDependencyPin": ":bug: Fix: Pin ${dependency} to ${oldVersion}"
    }
  }
}
