  "greenkeeper": {
    "commitMessages": {
      "initialBadge": ":memo: Docs: add Greenkeeper badge",
      "initialDependencies": ":gem: chore(deps): bump to latest",
      "initialBranches": ":tada: build: whitelist greenkeeper branches",
      "dependencyUpdate": ":gem: chore(deps): update ${dependency} to version ${version}",
      "devDependencyUpdate": ":gem: chore(deps): update ${dependency} to version ${version}",
      "dependencyPin": ":bug: chore(deps): pin ${dependency} to ${oldVersion}",
      "devDependencyPin": ":bug: chore(deps): pin ${dependency} to ${oldVersion}"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix",
      "yarn lint:css",
      "sh scripts/ts-precommit.bash",
      "yarn test --bail --findRelatedTests"
    ],
    "*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "**/**/set-value": ">=2.0.1",
    "**/**/js-yaml": ">=3.13.1"
  }
}
