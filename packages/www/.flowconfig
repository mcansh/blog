[ignore]
.*/node_modules/.*
.*/.next/.*
.*/next.config.js
.*/__tests__/.*

[include]

[libs]

[lints]
all=error

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
sketchy-null

[options]
all=true
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
esproposal.optional_chaining=enable
